{
    "name": "styxit/adventofcode2023",
    "type": "project",
    "authors": [
        {
            "name": "Styxit",
            "email": "info@styxit.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/console": "^7.0",
        "illuminate/collections": "^10.34"
    },
    "autoload": {
        "psr-4": {
            "Styxit\\": "./src",
            "Puzzles\\": "./puzzles"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "symfony/var-dumper": "^7.0",
        "friendsofphp/php-cs-fixer": "^3.40"
    },
    "scripts": {
        "csfixer": [
            "php vendor/bin/php-cs-fixer fix"
        ]
    }
}
